public class GameScoreImpl implements GameScore {

    /**
     * Default constructor. 
     */
    public GameScoreImpl() {
        // TODO Auto-generated constructor stub
    }

	@Override
	public String getScore(Team team, Game game) {
		String score = "";
		return score;
	}
    
    

}
